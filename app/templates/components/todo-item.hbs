<li>
  <div class="checkbox">
    <label>
      {{input type="checkbox"
          name="todoCheckBox"
          checked=model.isCompleted
          change=(action "toggleTodo")}}
      <span class="{{ if model.isCompleted 'strike-through' }}">
        {{ model.text }}
      </span>
      <span {{ action "removeTodo" }} class="remove">x</span>
    </label>
  </div>
</li>
